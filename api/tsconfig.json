{
  "compilerOptions": {
    "target": "ES2016",
    "module": "ESNext",  
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "moduleResolution": "Node",
    "outDir": "./dist",  // Assurez-vous que la sortie est configurée
    "rootDir": "./src"   // Spécifiez la racine de vos fichiers source
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules", "dist"]
}
